{
    "name": "built-fast/phpstan-sensitive-parameter",
    "description": "PHPStan rule to detect potential sensitive parameters",
    "type": "phpstan-extension",
    "license": "MIT",
    "version": "1.0.0",
    "require": {
        "php": "^8.0",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BuiltFast\\": "src/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "rules.neon"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "laravel/pint": "^1.22"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
